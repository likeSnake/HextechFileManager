<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="net.Hextech.supertool.useful.file.hextechfilemanager">
    <!-- 读写权限 -->
    <uses-permission
        android:name="android.permission.MANAGE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.HextechFileManager">
        <activity
            android:name=".activity.ZipAct"
            android:exported="true" />
        <activity
            android:name=".activity.VidoActivity"
            android:exported="true" />
        <activity
            android:name=".activity.ImageAct"
            android:exported="true" />
        <activity
            android:name=".activity.MainAct"
            android:exported="true" />
        <activity
            android:name=".activity.StartAct"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>